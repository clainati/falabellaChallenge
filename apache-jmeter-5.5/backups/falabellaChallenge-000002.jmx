            <stringProp name="script">import org.apache.commons.io.FileUtils
import java.text.SimpleDateFormat;
// Toma el total de resultados
def resultCount = vars.get(&quot;userNameExtraidos_matchNr&quot;)
//escribe en la consola de jmeter el resultado encontrados en el match
log.warn &apos;Output to jmeter console&apos; +  resultCount
//Genera una fecha para que el csv no se repita
String fileSuffix = new SimpleDateFormat(&quot;ddMMyyyyHHmm&quot;).format(new Date())
// Crea un objeto de la clase file
f = new File(&quot;results_&quot;+fileSuffix+&quot;.csv&quot;)
for (int i=1; i&lt;=resultCount.toInteger(); i++)
{
//obtiene los resultados extraidos para posteriormente escribir en el csv 
  records = vars.get(&quot;userNameExtraidos_&quot;+i)
// Escribe el resultado del csv
  FileUtils.writeStringToFile(f,records + System.getProperty(&quot;line.separator&quot;),true)
}</stringProp>
